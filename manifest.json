{
    "manifest_version": 3,
    "name": "Speedify",
    "description": "extensions by Quema",
    "version": "0.0.2",
    "icons": {
        "16": "img/Speedify.png",
        "48": "img/Speedify.png",
        "128": "img/Speedify.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "cookies",
        "declarativeContent",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://open.spotify.com/*"
            ],
            "js": [
                "injection.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "injectionfile.js",
                "main.js"
            ],
            "matches": [
                "https://open.spotify.com/*"
            ]
        }
    ]
}